{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "server.js"
    }
  ],
  "crons": [
    {
      "path": "/api/cron/cleanup-expired",
      "schedule": "0 */30 * * *"
    },
    {
      "path": "/api/cron/activate-queues",
      "schedule": "0 * * * *"
    }
  ]
}